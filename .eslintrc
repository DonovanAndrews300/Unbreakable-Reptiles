        "prettier", 
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
     ],
     "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    },
    "plugins": ["prettier", "jsx-a11y"],
    "rules": {
      "prettier/prettier": ["error"]
    }
}